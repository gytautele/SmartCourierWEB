@page
@model ManagementModel
@{ ViewData["Title"] = ""; }
<h1>@ViewData["Title"]</h1>

<div class="parallax-content baner-content" id="home">
    <span><em>Restore</em> your password</span>
    <div class="container">
        <div class="row">
            <div class="col-md-6">
                <div class="contact-form">
                    <div class="row">
                        <form id="contact" action="" method="post">
                            <div class="row">
                                <div class="col-md-12">
                                    <fieldset>
                                        <input name="dispatch_id_data" class="form-control" id="email" placeholder="E-mail address" required="">
                                    </fieldset>
                                </div>
                                <div class="col-md-12">
                                    <fieldset>
                                        <form method="post">
                                            <button type="submit" name="searchData" id="form-submit" class="btn" style="padding-top:15px; padding-bottom: 15px;">Reset</button>
                                        </form>                                                                               
                                    </fieldset>
                                </div>
                            </div>                            
                        </form>
                    </div>
                </div>
            </div>            
    </div>
</div>


